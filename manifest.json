{
    "name": "Form Extender",
    "description": "Allows easy access to unifed planner form",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/form16.png",
            "32": "/images/form32.png",
            "48": "/images/form48.png", 
            "128": "/images/form128.png"
        }
    }


}